package com.demo.test;

public class ReverseVowels {

	public static void main(String[] args) {
		  char[] c=Arrays.toCharArray(s);
	        int front=0;
	        int rear=c.length-1;

	        while(front<rear){
	            char temp1='';
	            char temp2='';
	            boolean f1=false;
	            boolean f2=false;
	            if(c[front]=='a'||c[front]=='e'||c[front]=='i'||c[front]=='o'||c[front]=='u'){
	                temp1=c[front];
	                f1=true;
	            }else{
	                front++;
	                f1=false;
	            }
	             if(c[rear]=='a'||c[rear]=='e'||c[rear]=='i'||c[rear]=='o'||c[rear]=='u'){
	                temp1=c[rear];
	                f2=true;
	            }else{
	                rear++;
	                f2=false;
	            }
	            if(f1==true && f2=true){
	                c[front]=temp2;
	                c[rear]=temp1;
	            }
	        }
	        
	    System.out.println(new String(c));
	  }

}
